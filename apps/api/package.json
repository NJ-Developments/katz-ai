    "@katzai/shared": "workspace:*",
    "@prisma/client": "^5.8.1",
    "bcryptjs": "^3.0.3",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "fastify-plugin": "^5.1.0",
    "openai": "^4.25.0",
    "pino": "^8.17.2",
